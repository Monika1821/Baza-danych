  -- 13. Zamówienie - chleb żytni (Kategoria: Pieczywo)
  v_produkt_lista := lista_produktow(typ_produktow('chleb żytni', 11));
  dodaj_zamowienie(p_produkt_lista => v_produkt_lista, p_on_off => 'gotowka', p_id_klienta => 13, p_id_pracownik => 13);

  -- 14. Zamówienie - bułki pszenne (Kategoria: Pieczywo)
  v_produkt_lista := lista_produktow(typ_produktow('bułki pszenne', 16));
  dodaj_zamowienie(p_produkt_lista => v_produkt_lista, p_on_off => 'gotowka', p_id_klienta => 14, p_id_pracownik => 14);

  -- 15. Zamówienie - czekolada mleczna (Kategoria: Słodycze)
  v_produkt_lista := lista_produktow(typ_produktow('czekolada mleczna', 25));
  dodaj_zamowienie(p_produkt_lista => v_produkt_lista, p_on_off => 'gotowka', p_id_klienta => 15, p_id_pracownik => 15);

END;
