DECLARE
  v_produkt_lista lista_produktow;

BEGIN
  -- 1. Zamówienie - woda mineralna (Kategoria: Napoje)
  v_produkt_lista := lista_produktow(typ_produktow('woda mineralna', 16));
  dodaj_zamowienie(p_produkt_lista => v_produkt_lista, p_on_off => 'gotowka', p_id_klienta => 1, p_id_pracownik => 1);

  -- 2. Zamówienie - sok pomarańczowy (Kategoria: Napoje)
  v_produkt_lista := lista_produktow(typ_produktow('sok pomarańczowy', 33));
  dodaj_zamowienie(p_produkt_lista => v_produkt_lista, p_on_off => 'gotowka', p_id_klienta => 2, p_id_pracownik => 2);

  -- 3. Zamówienie - mleko 3,2% (Kategoria: Nabiał)
