  -- 9. Zamówienie - pierś z kurczaka (Kategoria: Mięso)
  v_produkt_lista := lista_produktow(typ_produktow('pierś z kurczaka', 16));
  dodaj_zamowienie(p_produkt_lista => v_produkt_lista, p_on_off => 'gotowka', p_id_klienta => 9, p_id_pracownik => 9);

  -- 10. Zamówienie - schab wieprzowy (Kategoria: Mięso)
  v_produkt_lista := lista_produktow(typ_produktow('schab wieprzowy', 13));
  dodaj_zamowienie(p_produkt_lista => v_produkt_lista, p_on_off => 'gotowka', p_id_klienta => 10, p_id_pracownik => 10);

  -- 11. Zamówienie - łosoś filet (Kategoria: Ryby i owoce morza)
